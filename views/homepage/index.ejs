        <img src="/images/surfBoard.png" style="width: 10%; height: 10%;" />
      </h2>
      <!-- <p>~ GET ON YOUR WAVELINK ~</p> -->
      <div class="container">
        <div class="row align-items-center justify-content-center">
          <div class="sign-in">
            <form
              method="POST"
              action="/api/login"
              class="form-inline"
              id="log-in"
              novalidate="novalidate"
            >
              <div class="form-group">
                <input
                  type="email"
                  name="email"
                  class="form-control"
                  id="email"
                  placeholder="Email"
                  required=""
                  autofocus=""
                  aria-required="true"
                />
              </div>
              <div class="form-group">
                <input
                  type="password"
                  name="password"
                  class="form-control"
                  placeholder="Password"
                  required=""
                  aria-required="true"
                />
              </div>
              <div class="form-group">
                <input type="submit" value="Log In" class="btn btn-dark" />
              </div>
            </form>
          </div>
        </div>

        <p id="sign-in-link"><a href="/user/new">New Surfer Sign Up</a></p>
      </div>
    </div>

