    <!-- Landing message -->

    <div class="row landing-message">
      <h2>PartyWave</h2>
      <p>Find your waves</p>
      <div>
        <div class="row">
          <div class="col-md-12 sign-in">
            <form
              method="post"
              action="/api/login"
              class="form-inline"
              id="log-in"
              novalidate="novalidate"
            >
              <div class="form-group">
                <input
                  type="email"
                  name="user[email]"
                  class="form-control"
                  id="email"
                  placeholder="Email"
                  required=""
                  autofocus=""
                  aria-required="true"
                />
              </div>
              <div class="form-group">
                <input
                  type="password"
                  name="user[password]"
                  class="form-control"
                  placeholder="Password"
                  required=""
                  aria-required="true"
                />
              </div>
              <div class="form-group">
                <input type="submit" value="Log In" class="btn btn-dark" />
              </div>
            </form>
          </div>
        </div>
        <p id="sign-in-link">
          New user?
          <a href="" data-toggle="modal" data-target="#signUpModal"
            >Sign up here</a
          >
        </p>
      </div>
    </div>

